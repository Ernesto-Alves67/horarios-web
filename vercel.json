{
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/sw.js"
    },
    {
      "src": "/manifest.webmanifest",
      "headers": {
        "content-type": "application/manifest+json"
      },
      "dest": "/manifest.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
